  "layer": [
    {
      "mark": {
        "type": "area",
        "line": true,
        "point": true
      },
      "encoding": {
        "x": {
          "field": "date",
          "type": "temporal",
          "timeUnit": "year",
          "axis": {
            "title": "Year",
            "tickCount": 10,
            "titleFont": "Century Gothic",
            "labelFont": "Century Gothic"
          }
        },
        "y": {
          "aggregate": "count",
          "title": "Number of Cases",
